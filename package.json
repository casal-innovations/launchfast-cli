{
	"name": "@launchfasthq/cli",
	"version": "1.3.0",
	"description": "LaunchFast CLI - Authentication and orchestration for LaunchFast projects",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:unit": "vitest run --exclude 'tests/e2e/**'",
		"test:e2e": "vitest run tests/e2e",
		"prepublishOnly": "npm run build && mv README.md README.dev.md && cp README.npm.md README.md",
		"postpublish": "mv README.dev.md README.md",
		"prepare": "husky"
	},
	"sideEffects": false,
	"engines": {
		"node": "20"
	},
	"dependencies": {
		"ini": "^5.0.0",
		"zod": "^3.23.8"
	},
	"optionalDependencies": {
		"@launchfasthq/installer": "^1.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.3",
		"@semantic-release/npm": "^12.0.2",
		"@types/ini": "^4.1.1",
		"@types/node": "^20.11.0",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"husky": "^9.1.7",
		"semantic-release": "^24.2.9",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^2.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/casal-innovations/launchfast-cli.git"
	},
	"keywords": [
		"launchfast",
		"cli",
		"remix"
	],
	"author": "Casal Innovations",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}
